<div class="field">
  <%= f.attribute_fields do |a| %>
      <%= a.attribute_select associations: [:category] %>
  <% end %>
  <%= f.predicate_select %>
  <%= f.value_fields do |v| %>
      <%= v.text_field :value %>
  <% end %>
  <%= button_tag t("search_ransack.remove"), type: :button, class: "remove_fields" %>
</div>
